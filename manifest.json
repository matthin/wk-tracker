{
  "manifest_version": 2,
  "name": "WaniKani Tracker",
  "version": "0.0.2",

  "description": "Tracks WaniKani review sessions.",

  "content_scripts": [
    {
      "matches": ["https://www.wanikani.com/review"],
      "js": ["dst/index.js"]
    }
  ],

  "options_ui": {
    "page": "options.html"
  },

  "permissions": [
    "downloads",
    "storage"
  ]
}
